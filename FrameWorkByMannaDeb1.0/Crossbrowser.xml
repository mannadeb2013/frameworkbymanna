<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Testing" parallel="classes" thread-count="3">
<listeners>
<listener class-name="com.sauseDemo.listener.TestListener"></listener>
</listeners>
  <test  name="Chrome Test">
  <parameter name="browserType" value="Chrome"/>
    <classes>
      <class name="com.sausedemo.runner.Runner"/>
    </classes>
  </test> 
   <test  name="Firefox Test">
   <parameter name="browserType" value="firefox"/>
    <classes>
      <class name="com.sausedemo.runner.Runner"/>
    </classes>
  </test> 
  <test  name="Edge Test">
   <parameter name="browserType" value="edge"/>
    <classes>
      <class name="com.sausedemo.runner.Runner"/>
    </classes>
  </test> 
</suite> 
